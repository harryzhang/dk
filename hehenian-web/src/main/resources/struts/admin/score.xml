<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="score" extends="admin-default" namespace="/admin">
		
		<action name="queryScoreListInit" class="scoreAction" method="queryScoreListInit" >
			<result name="success" >/WEB-INF/page/admin/score_list_index.jsp</result>
		</action>
		<action name="queryScoreList" class="scoreAction" method="queryScoreList" >
			<result name="success" >/WEB-INF/page/admin/score_list_info.jsp</result>
		</action>
		
		<action name="queryScoreByID" class="scoreAction" method="queryScoreByID" >
			<result name="success" >/WEB-INF/page/admin/score_show.jsp</result>
		</action>
		<action name="exporScoreList" class="scoreAction" method="exporScoreList" >
		</action>
	</package>
</struts>
