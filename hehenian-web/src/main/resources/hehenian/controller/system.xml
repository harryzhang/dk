<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="system.settscheme" extends="admin-default"
		namespace="/admin">
		<action name="querySettSchemes" class="settSchemeAction"
			method="querySettSchemes">
			<result name="success">/WEB-INF/jsp/system/settscheme_list.jsp</result>
		</action>
		<action name="toSettSchemeDetail" class="settSchemeAction"
			method="toSettSchemeDetail">
			<result name="success">/WEB-INF/jsp/system/settscheme_detail.jsp</result>
		</action>
	</package>
</struts>